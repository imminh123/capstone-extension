{
   "name": "Capstone Extension",
   "version": "1.0",
   "description": "Extension",
   "permissions": ["webRequest", "webRequestBlocking",
      "*://*.coursera.org/*","*://noteitfu.herokuapp.com/*","storage"],
   "content_scripts": [
      {
      "matches": ["*://*.coursera.org/*","*://noteitfu.herokuapp.com/*"],
      "js": ["./ContentScripts/js/jquery-3.4.1.min.js","./ContentScripts/js/script.js"],
      "css": ["./ContentScripts/css/css.css"],
      "run_at": "document_start"
      }
   ],
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Show popup" 
   },
   "manifest_version": 2
 }